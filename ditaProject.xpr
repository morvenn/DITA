<?xml version="1.0" encoding="UTF-8"?>
<project>
    <meta>
        <filters directoryPatterns="" filePatterns="\QditaProject.xpr\E" positiveFilePatterns="" showHiddenFiles="false"/>
        <options>
            <serialized xml:space="preserve">
                <serializableOrderedMap>
                    <entry>
                        <String>additional.frameworks.directories</String>
                        <String-array>
                            <String>${pd}/frameworks</String>
                        </String-array>
                    </entry>
                    <entry>
                        <String>document.types</String>
                        <documentTypeDescriptor-array/>
                    </entry>
                    <entry>
                        <String>enable.project.master.files.support</String>
                        <Boolean>true</Boolean>
                    </entry>
                    <entry>
                        <String>key.author.editor.option.pane</String>
                        <Boolean>true</Boolean>
                    </entry>
                    <entry>
                        <String>key.editor.document.type.association.option.pane</String>
                        <Boolean>true</Boolean>
                    </entry>
                    <entry>
                        <String>key.editor.document.type.custom.locations.option.pane</String>
                        <Boolean>true</Boolean>
                    </entry>
                    <entry>
                        <String>key.publishing.option.pane</String>
                        <Boolean>true</Boolean>
                    </entry>
                    <entry>
                        <String>key.spellcheck.option.pane</String>
                        <Boolean>true</Boolean>
                    </entry>
                    <entry>
                        <String>keys.context</String>
                        <String>${pdu}/dita/Untitled.ditamap</String>
                    </entry>
                    <entry>
                        <String>perspectives.layout.info</String>
                        <perspectivesInfo>
                            <field name="useFixedLayout">
                                <Boolean>true</Boolean>
                            </field>
                            <field name="predefinedLayout">
                                <Boolean>false</Boolean>
                            </field>
                            <field name="predefinedLayoutName">
                                <String>Advanced</String>
                            </field>
                            <field name="specifiedLayoutPath">
                                <String>${pd}/layout/ditaEditing.layout</String>
                            </field>
                        </perspectivesInfo>
                    </entry>
                    <entry>
                        <String>publishing.templates.gallery.directories</String>
                        <String-array>
                            <String>${pd}/publishing</String>
                        </String-array>
                    </entry>
                    <entry>
                        <String>spell.check.options</String>
                        <spellCheckOptions>
                            <field name="automaticSpellCheck">
                                <Boolean>true</Boolean>
                            </field>
                            <field name="automaticSpellCheckContentTypes">
                                <String-array>
                                    <String>application/xhtml+xml</String>
                                    <String>text/html</String>
                                    <String>text/markdown</String>
                                    <String>text/nvdl</String>
                                    <String>text/plain</String>
                                    <String>text/properties</String>
                                    <String>text/rng</String>
                                    <String>text/sch</String>
                                    <String>text/wsdl</String>
                                    <String>text/xml</String>
                                    <String>text/xproc</String>
                                    <String>text/xsd</String>
                                    <String>text/xsl</String>
                                    <String>text/xspec</String>
                                </String-array>
                            </field>
                            <field name="spellCheckInComments">
                                <Boolean>false</Boolean>
                            </field>
                            <field name="spellCheckInPIs">
                                <Boolean>false</Boolean>
                            </field>
                            <field name="spellCheckInAttributeValues">
                                <Boolean>false</Boolean>
                            </field>
                            <field name="spellCheckInText">
                                <Boolean>true</Boolean>
                            </field>
                            <field name="spellCheckInCDATA">
                                <Boolean>true</Boolean>
                            </field>
                            <field name="language">
                                <String>en</String>
                            </field>
                            <field name="caseSensitive">
                                <Boolean>false</Boolean>
                            </field>
                            <field name="ignoreMixedCase">
                                <Boolean>false</Boolean>
                            </field>
                            <field name="ignoreAcronyms">
                                <Boolean>true</Boolean>
                            </field>
                            <field name="ignoreDigits">
                                <Boolean>true</Boolean>
                            </field>
                            <field name="ignoreDuplicates">
                                <Boolean>false</Boolean>
                            </field>
                            <field name="ignoreURL">
                                <Boolean>true</Boolean>
                            </field>
                            <field name="checkPunctuation">
                                <Boolean>false</Boolean>
                            </field>
                            <field name="allowCompound">
                                <Boolean>true</Boolean>
                            </field>
                            <field name="allowFileExtensions">
                                <Boolean>true</Boolean>
                            </field>
                            <field name="obeyLangAttributes">
                                <Boolean>true</Boolean>
                            </field>
                            <field name="langAttrMissingUseDefault">
                                <Boolean>true</Boolean>
                            </field>
                            <field name="ignoreSpellCheckElements">
                                <String-array/>
                            </field>
                            <field name="preferredChecker">
                                <Integer>1</Integer>
                            </field>
                        </spellCheckOptions>
                    </entry>
                    <entry>
                        <String>tagless.editor.tags.display.mode</String>
                        <Integer>0</Integer>
                    </entry>
                    <entry>
                        <String>user.custom.templates.directories</String>
                        <String-array>
                            <String>${pd}/templates</String>
                        </String-array>
                    </entry>
                </serializableOrderedMap>
            </serialized>
        </options>
    </meta>
    <projectTree name="ditaProject.xpr">
        <folder masterFiles="true" name="Master Files">
            <file name="src/userguide.ditamap"/>
        </folder>
        <folder path="."/>
    </projectTree>
</project>